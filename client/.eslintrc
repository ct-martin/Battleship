{
    "extends": "airbnb/base",

    "parserOptions": {
        "ecmaFeatures": {
            "impliedStrict": true
        },
        "sourceType": "script"
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
      "no-underscore-dangle" : "off",
      "no-plusplus": "off"
    }
  }
  